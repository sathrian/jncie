            rule-set UNTRUST {
                from zone [ INTERNAL TRUST WAREHOUSE ];
                to zone UNTRUST;
                rule WEBSENSE {
                    match {
                        source-address 172.16.60.99/32;
                        destination-address 0.0.0.0/0;
