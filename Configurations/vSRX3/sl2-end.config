services {
    rpm {
        probe ping-9 {
            test icmp {
                probe-type icmp-ping;
                target address 12.34.56.78;
                probe-count 3;
                probe-interval 5;
                test-interval 15;
                thresholds {
                    successive-loss 3;
                    total-loss 3;
                }
                next-hop 80.1.1.9;
            }
        }
        probe ping-13 {
            test icmp {
                probe-type icmp-ping;
                target address 12.34.56.78;
                probe-count 3;
                probe-interval 5;
                test-interval 15;
                thresholds {
                    successive-loss 3;
                    total-loss 3;
                }
                next-hop 80.1.1.13;
            }
        }
    }
    application-identification {
        no-application-system-cache;
    }
    ssl {
        proxy {
            profile CENTRAL-SSL {
                root-ca MY-CERT;
                actions {
                    ignore-server-auth-failure;
                    log {
                        all;
                    }
                }
            }
        }
    }
    ip-monitoring {
        policy ping-9 {
            match {
                rpm-probe ping-9;
            }
            then {
                interface ge-0/0/3 {
                    disable;
                }
            }
        }
        policy ping-13 {
            match {
                rpm-probe ping-13;
            }
            then {
                interface ge-7/0/3 {
                    disable;
                }
            }
        }
    }
}
security {
    ike {
        proposal IKE-PROP-HOFFICE {
            authentication-method pre-shared-keys;
            dh-group group1;
            authentication-algorithm md5;
            encryption-algorithm des-cbc;
        }
        proposal IKE-PROP-HUB-SPOKE {
            authentication-method pre-shared-keys;
            dh-group group2;
            authentication-algorithm sha1;
            encryption-algorithm aes-128-cbc;
            lifetime-seconds 57600;
        }
        policy IKE-POL-HOFFICE {
            mode main;
            proposals IKE-PROP-HOFFICE;
            pre-shared-key ascii-text "$9$IUwhyKX7V4aUM8aUjH5TRhS"; ## SECRET-DATA
        }
        policy IKE-POL-HUB-SPOKE {
            mode main;
            proposals IKE-PROP-HUB-SPOKE;
            pre-shared-key ascii-text "$9$sY2oGzF/u0I69xdbsJZn/Cu1h"; ## SECRET-DATA
        }
        policy IKE-POL-BASIC {
            mode aggressive;
            proposal-set basic;
            pre-shared-key ascii-text "$9$pTmG01h7Nb4oGVw39ApREdbs4Zj"; ## SECRET-DATA
        }
        gateway TO-HOFFICE {
            ike-policy IKE-POL-HOFFICE;
            address 200.1.1.100;
            external-interface lo0.0;
        }
        gateway TO-ROFFICE2 {
            ike-policy IKE-POL-HUB-SPOKE;
            address 80.1.1.5;
            external-interface lo0.0;
        }
        gateway TO-EXA {
            ike-policy IKE-POL-HUB-SPOKE;
            address 100.1.1.1;
            external-interface lo0.0;
        }
        gateway TO-ROFFICE1 {
            ike-policy IKE-POL-BASIC;
            dynamic hostname roffice1;
            external-interface lo0.0;
        }
    }
    ipsec {
        vpn-monitor-options {
            interval 5;
            threshold 3;
        }
        proposal IPSEC-PROP-HOFFICE {
            protocol esp;
            authentication-algorithm hmac-md5-96;
            encryption-algorithm des-cbc;
        }
        proposal IPSEC-PROP-HUB-SPOKE {
            protocol esp;
            authentication-algorithm hmac-sha1-96;
            encryption-algorithm aes-192-cbc;
            lifetime-kilobytes 100000;
        }
        policy IPSEC-POL-HOFFICE {
            proposals IPSEC-PROP-HOFFICE;
        }
        policy IPSEC-POL-HUB-SPOKE {
            proposals IPSEC-PROP-HUB-SPOKE;
        }
        policy IPSEC-POL-BASIC {
            proposal-set basic;
        }
        vpn TO-HOFFICE {
            bind-interface st0.1;
            ike {
                gateway TO-HOFFICE;
                proxy-identity {
                    local 172.16.0.0/16;
                    remote 172.16.77.0/24;
                    service any;
                }
                ipsec-policy IPSEC-POL-HOFFICE;
            }
            establish-tunnels immediately;
        }
        vpn TO-ROFFICE2 {
            bind-interface st0.0;
            vpn-monitor;
            ike {
                gateway TO-ROFFICE2;
                ipsec-policy IPSEC-POL-HUB-SPOKE;
            }
            establish-tunnels immediately;
        }
        vpn TO-EXA {
            bind-interface st0.0;
            vpn-monitor;
            ike {
                gateway TO-EXA;
                ipsec-policy IPSEC-POL-HUB-SPOKE;
            }
            establish-tunnels immediately;
        }
        vpn TO-ROFFICE1 {
            bind-interface st0.2;
            ike {
                gateway TO-ROFFICE1;
                ipsec-policy IPSEC-POL-BASIC;
            }
        }
    }
    idp {
        idp-policy CENTRAL-IDP {
            rulebase-ips {
                rule FILE-ATTACK {
                    match {
                        from-zone any;
                        source-address any;
                        to-zone WAREHOUSE;
                        destination-address WAREHOUSE-SRV-206;
                        application junos-ftp;
                        attacks {
                            custom-attacks FILE-ATTACK;
                        }
                    }
                    then {
                        action {
                            drop-connection;
                        }
                    }
                }
                rule ANONYMOUS-ATTACK {
                    match {
                        from-zone any;
                        source-address any;
                        to-zone WAREHOUSE;
                        destination-address WAREHOUSE-SRV-206;
                        application junos-ftp;
                        attacks {
                            custom-attacks ANONYMOUS-ATTACK;
                        }
                    }
                    then {
                        action {
                            drop-packet;
                        }
                        ip-action {
                            ip-block;
                            target source-address;
                            timeout 3600;
                        }
                        notification {
                            log-attacks {
                                alert;
                            }
                        }
                    }
                }
            }
            rulebase-exempt {
                rule FILE-ATTACK {
                    match {
                        from-zone TRUST;
                        source-address any;
                        to-zone WAREHOUSE;
                        destination-address WAREHOUSE-SRV-206;
                        attacks {
                            custom-attacks FILE-ATTACK;
                        }
                    }
                }
                rule ANONYMOUS-TRUST {
                    match {
                        from-zone TRUST;
                        source-address any;
                        to-zone WAREHOUSE;
                        destination-address WAREHOUSE-SRV-206;
                        attacks {
                            custom-attacks ANONYMOUS-ATTACK;
                        }
                    }
                }
                rule ANONYMOUS-TESTBED {
                    match {
                        from-zone TESTBED;
                        source-address any;
                        to-zone WAREHOUSE;
                        destination-address WAREHOUSE-SRV-206;
                        attacks {
                            custom-attacks ANONYMOUS-ATTACK;
                        }
                    }
                }
            }
        }
        custom-attack FILE-ATTACK {
            severity major;
            attack-type {
                chain {
                    expression "exe or rpm";
                    member exe {
                        attack-type {
                            signature {
                                context ftp-put-filename;
                                pattern ".*\.\[exe\]";
                                direction client-to-server;
                            }
                        }
                    }
                    member rpm {
                        attack-type {
                            signature {
                                context ftp-put-filename;
                                pattern ".*\.\[rpm\]";
                                direction client-to-server;
                            }
                        }
                    }
                }
            }
        }
        custom-attack ANONYMOUS-ATTACK {
            severity major;
            attack-type {
                signature {
                    context ftp-username;
                    pattern "^anonymous$";
                    direction client-to-server;
                }
            }
        }
    }
