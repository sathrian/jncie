firewall {
    family inet {
        filter RE-PROTECT-v4 {
            term DNS {
                from {
                    source-address {
                        8.8.8.8/32;
                    }
                }
                then accept;
            }
            term sdsn {
                from {
                    protocol tcp;
                    port [ https http 8080 ];
                }
                then accept;
            }
            term IFD {
                from {
                    interface gr-0/0/0;
                    interface st0;
                }
                then accept;
            }
            term PORT-TCP {
                from {
                    source-prefix-list {
                        mgmt;
                    }
                    protocol tcp;
                    destination-port [ https ssh ];
                }
                then accept;
            }
            term LOG-TCP {
                from {
                    protocol tcp;
                    destination-port [ https ssh ];
                }
                then {
                    count LOG-TCP;
                    discard;
                }
            }
            term ICMP {
                from {
                    protocol icmp;
                }
                then {
                    policer ICMP-POLICER;
                    accept;
                }
            }
            term SNMP {
                from {
                    source-prefix-list {
                        mgmt;
                    }
                    port [ snmp snmptrap ];
                }
                then {
                    count SNMP;
                    accept;
                }
            }
            term BGP {
                from {
                    prefix-list {
                        bgp;
                    }
                    protocol tcp;
                    port bgp;
                }
                then {
                    count BGP;
                    accept;
                }
            }
            term OSPF {
                from {
                    protocol ospf;
                }
                then {
                    count OSPF;
                    accept;
                }
            }
            term LDP {
                from {
                    port ldp;
                }
                then {
                    count LDP;
                    accept;
                }
            }
            term IKE {
                from {
                    port 500;
                }
                then {
                    count IKE;
                    accept;
                }
            }
            term PORT-UDP {
                from {
                    source-prefix-list {
                        mgmt;
                    }
                    protocol udp;
                    port [ domain ntp ];
                }
                then accept;
            }
            term LOG-UDP {
                from {
                    protocol udp;
                    port [ domain ntp ];
                }
                then {
                    count LOG-UDP;
                    discard;
                }
            }
        }
        filter FBF-WEBSENSE {
            term http {
                from {
                    protocol tcp;
                    destination-port http;
                }
                then {
                    routing-instance websense;
                }
            }
            term else {
                then accept;
            }
        }
    }
    family inet6 {
        filter RE-PROTECT-v6 {
            term IFD {
                from {
                    interface gr-0/0/0;
                    interface st0;
                }
                then accept;
            }
            term SRC-ALLOW {
                from {
                    source-address {
                        2016:abcd::/32;
                    }
                    source-port [ domain ntp ];
                }
                then accept;
            }
            term SRC-DENY {
                from {
                    source-port [ domain ntp ];
                }
                then count SRC-DENY;
            }
            term DST-ALLOW {
                from {
                    source-address {
                        2016:abcd::/32;
                    }
                    destination-port [ https ssh ];
                }
                then accept;
            }
            term DST-DENY {
                from {
                    destination-port [ https ssh ];
                }
                then count DST-DENY;
            }
            term BGP {
                from {
                    prefix-list {
                        bgp;
                    }
                    port bgp;
                }
                then {
                    count BGP;
                    accept;
                }
            }
            term ICMP {
                from {
                    icmp-type [ echo-reply echo-request neighbor-advertisement neighbor-solicit ];
                }
                then {
                    count ICMP;
                    accept;
                }
            }
        }
    }
    policer ICMP-POLICER {
        if-exceeding {
            bandwidth-limit 256k;
            burst-size-limit 100k;
        }
        then discard;
    }
}
